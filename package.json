    "cz-conventional-changelog": "^3.1.0",
    "electron": "^8.2.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "node-sass": "^4.14.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-app-rewired": "^2.1.5",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
